<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
		<string>public.objective-c-source</string>
		<string>public.objective-c-plus-plus-source</string>
	</array>

  <key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>

  <key>Description</key>
	<string>Code file organized with Xcode Macros and comments. Includes Swift and Objective-C versions.</string>

  <key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>

  <key>DefaultCompletionName</key>
	<string>MyClass</string>
	<!--
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.m</string>
	-->
  <key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>The name of the class to create</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Name</key>
			<string>File's Name:</string>
			<key>NotPersisted</key>
			<true/>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>languageChoice</string>
			<key>Description</key>
			<string>The code's language</string>
			<key>Name</key>
			<string>Code's Language:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>popup</string>
			<key>Default</key>
			<string>Swift</string>
			<key>Values</key>
			<array>
				<string>Swift</string>
				<string>Objective-C</string>
			</array>
			<key>Units</key>
			<dict>
					<key>Objective-C</key>
					<dict>
							<key>Nodes</key>
							<array>
									<string>___FILEBASENAME___.m</string>
							</array>
							<key>Definitions</key>
							<dict>
									<key>___FILEBASENAME___.m</key>
									<dict>
											<key>Path</key>
											<string>___FILEBASENAME___.m</string>
									</dict>
							</dict>
					</dict>
					<key>Swift</key>
					<dict>
							<key>Nodes</key>
							<array>
									<string>___FILEBASENAME___.swift</string>
							</array>
							<key>Definitions</key>
							<dict>
									<key>___FILEBASENAME___.swift</key>
									<dict>
											<key>Path</key>
											<string>___FILEBASENAME___.swift</string>
									</dict>
							</dict>
					</dict>
			</dict>
		</dict>

		<dict>
			<key>Description</key>
			<string>What class to subclass in the new file</string>
			<key>Identifier</key>
			<string>cocoaSubclass</string>
			<key>Name</key>
			<string>Subclass of:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>combo</string>
			<key>Default</key>
			<string>NSObject</string>
			<key>Values</key>
			<array>
				<string>NSObject</string>
				<string>UIView</string>
				<string>UIViewController</string>
				<string>NPPModelVO</string>
			</array>
		</dict>
	</array>

    <key>SortOrder</key>
		<string>1</string>

    <key>Summary</key>
	<string>Code file with Swift or Objective-C language</string>
</dict>
</plist>
